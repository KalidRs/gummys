        <div data-role="content">
        <form>
            <input data-type="search" id="searchForCollapsibleSet">
        </form>
        <div data-role="collapsibleset" data-filter="true" data-inset="true" id="collapsiblesetForFilter"
                data-input="#searchForCollapsibleSet">
                <% products.forEach((product)=>{ %>
                    <div data-role="collapsible" data-filtertext="<%= product.description%>">
                        <h3><%= product.description%></h3>
                        <ul data-role="listview" data-inset="false">
                            <li>Codigo de Barras: <%= product.barcode%></li>
                            <li>Descripcion: <%= product.description%></li>
                            <li>Precio de compra: <%= product.price%></li>
                            
                            <!-- <li><a href="<%= product.barcode%>" class="ui-btn">Editar</a></li> -->
                            <!-- <li><a href="/eliminar/<%= product.barcode%>" class="ui-btn ui-btn-b">Eliminar</a> -->
                            </li>
                        </ul>
                    </div>
